{
  "name": "nx-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "nx run-many --target=serve --all --parallel",
    "build": "nx run-many --target=build --all --parallel",
    "build:affected": "nx affected --target=build --parallel",
    "lint": "nx run-many --target=lint --all --parallel",
    "lint:affected": "nx affected --target=lint --parallel",
    "test": "nx run-many --target=test --all --parallel",
    "test:affected": "nx affected --target=test --parallel",
    "test:coverage": "nx affected --target=test --all --parallel --coverage",
    "e2e": "nx run-many --target=e2e --all",
    "e2e:affected": "nx affected --target=e2e --all",
    "e2e:nest": "start-server-and-test 'nx run nest:build && node dist/apps/nest/main.js' 3333/api e2e",
    "e2e:nest:affected": "start-server-and-test 'nx run nest:build && node dist/apps/nest/main.js' 3333/api e2e:affected",
    "format": "nx format",
    "format:check": "nx format:check --all",
    "format:check:affected": "nx format:check",
    "update": "nx migrate latest",
    "migrate": "nx migrate --run-migrations"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^13.3.1",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.10.2",
    "@nrwl/eslint-plugin-nx": "13.10.2",
    "@nrwl/jest": "13.10.2",
    "@nrwl/linter": "13.10.2",
    "@nrwl/nest": "^13.10.2",
    "@nrwl/node": "13.10.2",
    "@nrwl/workspace": "13.10.2",
    "@nxext/svelte": "^13.3.0",
    "@nxext/sveltekit": "^13.0.0-next.2",
    "@playwright/test": "^1.21.0-alpha-apr-2-2022",
    "@rgossiaux/svelte-headlessui": "^1.0.0-beta.12",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.37",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jest": "27.0.2",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "autoprefixer": "^10.4.4",
    "c8": "^7.11.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "jest": "27.2.3",
    "jest-esm-transformer": "^1.0.0",
    "nx": "13.10.2",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "start-server-and-test": "^1.14.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.4.6",
    "svelte-hero-icons": "^4.1.1",
    "svelte-jester": "^2.3.1",
    "svelte-preprocess": "^4.9.4",
    "tailwindcss": "^3.0.23",
    "ts-jest": "27.0.5",
    "typescript": "^4.6.3",
    "vitest": "^0.6.0"
  },
  "workspaces": [
    "apps/**",
    "packages/**"
  ],
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.0.0"
  }
}
