{
  "root": "apps/sveltekit",
  "sourceRoot": "apps/sveltekit/src",
  "projectType": "application",
  "tags": ["scope:app"],
  "targets": {
    "sync": {
      "executor": "@nxext/sveltekit:sveltekit",
      "options": {
        "command": "sync"
      }
    },
    "build": {
      "executor": "@nxext/sveltekit:sveltekit",
      "options": {
        "command": "build"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "serve": {
      "executor": "@nxext/sveltekit:sveltekit",
      "options": {
        "command": "dev"
      },
      "dependsOn": [
        {
          "target": "sync",
          "projects": "self"
        }
      ]
    },
    "preview": {
      "executor": "@nxext/sveltekit:sveltekit",
      "options": {
        "command": "preview"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    },
    "add": {
      "executor": "@nxext/sveltekit:add"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/sveltekit/**/*.{ts,svelte,spec.ts}"],
        "ignorePath": "apps/sveltekit/.gitignore"
      }
    },
    "check": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "svelte-check",
        "cwd": "apps/sveltekit",
        "tsconfig": "./tsconfig.app.json"
      }
    },
    "e2e": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "playwright test",
        "cwd": "apps/sveltekit"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "commitMessageFormat": "release(${projectName}): bump to ${version} [skip ci]",
        "tagPrefix": "${projectName}@v",
        "trackDeps": true
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    }
  }
}
